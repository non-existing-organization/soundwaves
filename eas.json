{
  "cli": {
    "version": ">= 3.15.0",
    "requireCommit": false,
    "promptToConfigurePushNotifications": false,
    "appVersionSource": "remote"
  },
  "build": {
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "ios": {
        "image": "latest"
      },
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "releaseChannel": "production",
      "ios": {
        "image": "latest"
      }
    },
    "production-ios": {
      "ios": {
        "image": "latest"
      },
      "distribution": "store",
      "releaseChannel": "production.ios-1.7.1"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "stilia.johny@gmail.com",
        "ascAppId": "8943PTRNBX",
        "appleTeamId": "73ET9TF83J",
      }
    }
  }
}
